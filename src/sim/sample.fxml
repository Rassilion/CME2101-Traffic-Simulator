<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.control.cell.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.cell.PropertyValueFactory?>


<TabPane fx:id="window" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
         prefHeight="623.0" prefWidth="911.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/null"
         xmlns:fx="http://javafx.com/fxml/1" fx:controller="sim.Controller">

    <tabs>
        <Tab text="Main">
            <content>
                <HBox fx:id="main" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                    <children>
                        <VBox alignment="CENTER">
                            <children>
                                <ScrollPane VBox.vgrow="SOMETIMES">
                                    <content>
                                        <AnchorPane maxHeight="5480.0" maxWidth="6340.0" minHeight="0.0" minWidth="0.0"
                                                    prefHeight="5180.0" prefWidth="6340.0">
                                            <children>
                                                <Canvas fx:id="layer1" height="5480.0" width="6340.0"/>
                                                <Canvas fx:id="layer2" height="5480.0" width="6340.0"/>
                                                <Canvas fx:id="layer3" height="5480.0" width="6340.0"/>
                                            </children>
                                        </AnchorPane>
                                    </content>
                                    <VBox.margin>
                                        <Insets/>
                                    </VBox.margin>
                                </ScrollPane>
                                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                                    <fx:define>
                                        <ToggleGroup fx:id="heuristicToggle"/>
                                    </fx:define>
                                    <children>
                                        <RadioButton fx:id="heuristic1" mnemonicParsing="false" selected="true"
                                                     text="Heuristic1">
                                            <toggleGroup>
                                                <ToggleGroup fx:id="heuristicToggle"/>
                                            </toggleGroup>
                                        </RadioButton>
                                        <RadioButton fx:id="heuristic2" mnemonicParsing="false" text="Heuristic2"
                                                     toggleGroup="$heuristicToggle"/>
                                        <Button fx:id="Movebutton" alignment="CENTER" mnemonicParsing="false"
                                                onAction="#ButtonClick" onKeyPressed="#ButtonKey" text="Move !!"/>
                                        <Button mnemonicParsing="false" onAction="#newButtonClick" text="New Sim"/>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                        <VBox alignment="CENTER_RIGHT" fillWidth="false" HBox.hgrow="SOMETIMES">
                            <children>
                                <HBox prefHeight="35.0" prefWidth="302.0" VBox.vgrow="ALWAYS">
                                    <children>
                                        <Label alignment="CENTER" contentDisplay="RIGHT" prefHeight="53.0"
                                               prefWidth="303.0" text="Time">
                                            <graphic>
                                                <TextField fx:id="timeArea" text="0"/>
                                            </graphic>
                                        </Label>
                                    </children>
                                </HBox>
                                <TableView fx:id="carList" VBox.vgrow="ALWAYS">
                                    <columns>
                                        <TableColumn fx:id="carName" editable="false" prefWidth="75.0" text="Name">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="name"/>
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn fx:id="current" editable="false" prefWidth="64.0" text="Current">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="currentNode"/>
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn fx:id="previous" editable="false" prefWidth="64.0" text="Previous">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="previousNode"/>
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn editable="false" prefWidth="64.0" text="Start">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="startingPoint"/>
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn editable="false" prefWidth="64.0" text="Wait">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="sumWait"/>
                                            </cellValueFactory>
                                        </TableColumn>
                                    </columns>
                                </TableView>
                                <TableView fx:id="ambulanceList" VBox.vgrow="ALWAYS">
                                    <columns>
                                        <TableColumn fx:id="ambulanceName" editable="false" prefWidth="75.0"
                                                     text="Name">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="name"/>
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn editable="false" prefWidth="64.0" text="Start">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="startingPoint"/>
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn editable="false" prefWidth="64.0" text="Dest">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="destination"/>
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn editable="false" prefWidth="64.0" text="Time">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="startTime"/>
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn editable="false" prefWidth="86.0" text="Current">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="currentNode"/>
                                            </cellValueFactory>
                                        </TableColumn>
                                    </columns>
                                </TableView>
                            </children>
                            <HBox.margin>
                                <Insets left="360.0"/>
                            </HBox.margin>
                            <padding>
                                <Insets left="-360.0"/>
                            </padding>
                        </VBox>
                    </children>
                </HBox>
            </content>
        </Tab>
        <Tab text="Statistics">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <GridPane prefHeight="586.0" prefWidth="911.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <BarChart fx:id="barChart1" barGap="1.0" categoryGap="1.0" prefHeight="225.0"
                                          prefWidth="381.0" title="Wait Times Cars">
                                    <xAxis>
                                        <CategoryAxis side="BOTTOM"/>
                                    </xAxis>
                                    <yAxis>
                                        <NumberAxis side="LEFT"/>
                                    </yAxis>
                                </BarChart>
                                <BarChart fx:id="barChart2" barGap="1.0" categoryGap="1.0" prefHeight="257.0"
                                          prefWidth="384.0" title="Ambulance Arriving Time" GridPane.rowIndex="1">
                                    <xAxis>
                                        <CategoryAxis side="BOTTOM"/>
                                    </xAxis>
                                    <yAxis>
                                        <NumberAxis side="LEFT"/>
                                    </yAxis>
                                </BarChart>
                                <BarChart fx:id="barChart3" barGap="1.0" categoryGap="1.0" prefHeight="257.0"
                                          prefWidth="381.0" title="Node Times" GridPane.columnIndex="1">
                                    <xAxis>
                                        <CategoryAxis side="BOTTOM"/>
                                    </xAxis>
                                    <yAxis>
                                        <NumberAxis side="LEFT"/>
                                    </yAxis>
                                </BarChart>
                            </children>
                        </GridPane>
                    </children>
                </AnchorPane>
            </content>
        </Tab>
        <Tab text="BackUp">
            <content>
                <Pane prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <Canvas fx:id="backUpCanvas" height="548.0" width="634.0"/>
                        <DatePicker fx:id="datePicker" layoutX="679.0" layoutY="40.0" onAction="#ChooseDate"
                                    prefHeight="31.0" prefWidth="209.0"/>
                        <ComboBox fx:id="simulationCombo" layoutX="685.0" layoutY="116.0" onAction="#ChooseSim"
                                  prefHeight="31.0" prefWidth="209.0" promptText="Select Simulation"/>
                        <ComboBox fx:id="timeCombo" layoutX="685.0" layoutY="181.0" prefHeight="31.0" prefWidth="209.0"
                                  promptText="Select Time"/>
                        <Button fx:id="backupButton" layoutX="751.0" layoutY="277.0" mnemonicParsing="false"
                                onAction="#BackUpButton" text="Back Up!!"/>
                    </children>
                </Pane>
            </content>
        </Tab>

    </tabs>
</TabPane>
